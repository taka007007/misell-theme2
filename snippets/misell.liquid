{% if font_family == 'Noto Sans JP', sans-serif %}
  <style>
  @import url('https://fonts.googleapis.com/css2?family=Noto+Sans+JP:wght@400;700&display=swap');
  </style>
  {% elsif font_family == 'Noto Serif JP', serif %}
  <style>
  @import url('https://fonts.googleapis.com/css2?family=Noto+Serif+JP:wght@400;700&display=swap');
  </style>
  {% elsif font_family == 'Zen Kaku Gothic New', sans-serif %}
  <style>
  @import url('https://fonts.googleapis.com/css2?family=Zen+Kaku+Gothic+New:wght@400;700&display=swap');
  </style>
  {% elsif font_family == 'Zen Maru Gothic', sans-serif %}
  <style>
  @import url('https://fonts.googleapis.com/css2?family=Zen+Maru+Gothic:wght@300;400;500;700&display=swap');
  </style>
  {% elsif font_family == 'Zen Old Mincho', serif %}
  <style>
  @import url('https://fonts.googleapis.com/css2?family=Zen+Old+Mincho:wght@400;700&display=swap');
  </style>
{% elsif font_family == 'M PLUS 1p', sans-serif %}
  <style>
  @import url('https://fonts.googleapis.com/css2?family=M+PLUS+1p:wght@300;400;700&display=swap');
  </style>
{% elsif font_family == 'M PLUS Rounded 1c', sans-serif %}
  <style>
  @import url('https://fonts.googleapis.com/css2?family=M+PLUS+Rounded+1c:wght@300;400;700&display=swap');
  </style>

{% elsif font_family == 'Shippori Mincho', serif %}
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Shippori+Mincho:wght@400;500;700;800&display=swap');
  </style>
{% elsif font_family == 'Sawarabi Gothic', sans-serif %}
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Sawarabi+Gothic:wght@400;700&display=swap');
  </style>
{% elsif font_family == 'Sawarabi Mincho', serif %}
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Sawarabi+Mincho:wght@400;700&display=swap');
  </style>
{% elsif font_family == 'Hina Mincho', serif %}
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Hina+Mincho:wght@400;700&display=swap');
  </style>
{% elsif font_family == 'Kaisei Opti', sans-serif %}
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Kaisei+Opti:wght@400;500;700&display=swap');
  </style>
{% elsif font_family == 'Kiwi Maru', display %}
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Kiwi+Maru&display=swap');
  </style>
  {% elsif font_family == 'Kosugi Maru', sans-serif %}
  <style>
    @import url('https://fonts.googleapis.com/css2?family=Kosugi+Maru&display=swap');
  </style>
  {% endif %}
  {% if settings.font_all_apply_to_font_feature %}
    <script src="https://unpkg.com/budoux/bundle/budoux-ja.min.js"></script>
  {% endif %}
